{
    "name": "codewow/filament-exact",
    "version": "2.0.1",
    "description": "ExactOnline Panel for FilamentPHP",
    "keywords": [
        "creativework",
        "codewow",
        "codewownl",
        "laravel",
        "filament-exact"
    ],
    "homepage": "https://github.com/codewownl/filament-exact",
    "support": {
        "issues": "https://github.com/codewownl/filament-exact/issues",
        "source": "https://github.com/codewownl/filament-exact"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jessedev1",
            "email": "jesse@codewow.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "bezhansalleh/filament-shield": "^4.1.0",
        "filament/filament": "^4.0|^5.0",
        "guzzlehttp/guzzle": "^7.9",
        "spatie/laravel-package-tools": "^1.92.0",
        "spatie/laravel-permission": "^6.24"
    },
    "require-dev": {
        "filament/upgrade": "^4.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.3",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "CodeWOW\\FilamentExact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWOW\\FilamentExact\\FilamentExactServiceProvider"
            ],
            "aliases": {
                "FilamentExact": "CodeWOW\\FilamentExact\\Facades\\FilamentExact"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
